<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>mobile-installer@xulforge.com</em:id>
    <em:name>Mobile Installer</em:name>
    <em:description>
      Facilitates installing XPIs in Firefox for Android.
    </em:description>
    <em:version>0.2</em:version>
    <em:creator>Jorge Villalobos</em:creator>
    <em:bootstrap>true</em:bootstrap>
    <em:type>2</em:type>
    <em:homepageURL>https://github.com/jvillalobos/Mobile-Installer</em:homepageURL>

    <!-- Mozilla Firefox for Android -->
    <em:targetApplication>
      <Description>
        <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
        <em:minVersion>16.0</em:minVersion>
        <em:maxVersion>18.*</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>
</RDF>
